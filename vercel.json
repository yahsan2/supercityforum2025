{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/",
      "destination": "/tiiki/kokusentoc/supercity/supercityforum2026/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/tiiki/kokusentoc/supercity/supercityforum2026/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/tiiki/kokusentoc/supercity/supercityforum2026/sousei/(.*)",
      "destination": "/sousei/$1"
    },
    {
      "source": "/tiiki/kokusentoc/supercity/supercityforum2026/",
      "destination": "/index.html"
    },
    {
      "source": "/tiiki/kokusentoc/supercity/supercityforum2026/:path*.html",
      "destination": "/:path*.html"
    }
  ],
  "headers": [
    {
      "source": "/tiiki/kokusentoc/supercity/supercityforum2026/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
